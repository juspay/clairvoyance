# Original environment variables (keep these as before)
GEMINI_API_KEY=your_gemini_api_key
DAILY_API_KEY=your_daily_api_key
AZURE_OPENAI_API_KEY=your_azure_openai_key
AZURE_OPENAI_ENDPOINT=your_azure_endpoint
GOOGLE_CREDENTIALS_JSON=your_google_credentials_json
ELEVENLABS_API_KEY=your_elevenlabs_key

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_FROM_NUMBER=your_twilio_phone_number
TWILIO_WEBSOCKET_URL=wss://your-app.com/agent/voice/breeze-buddy/twillio/callback/order-confirmation

# New Redis Configuration for Worker Pool Architecture
REDIS_URL=redis://localhost:6379
REDIS_SESSION_QUEUE=voice_sessions
REDIS_WORKER_RESPONSE_QUEUE=worker_responses
REDIS_CACHE_TTL=3600

# Worker Pool Configuration
WORKER_POOL_SIZE=4
MAX_SESSIONS_PER_WORKER=10
WORKER_HEARTBEAT_INTERVAL=30

# Redis Cleanup Configuration
WORKER_REGISTRATION_TTL=60
SESSION_ASSIGNMENT_TTL=300
HEARTBEAT_TTL=30
CLEANUP_INTERVAL=60
ENABLE_STARTUP_CLEANUP=true

# Optional: Performance Tuning
PING_INTERVAL=5
WS_PING_TIMEOUT=10
ENABLE_SUMMARIZATION=true
MAX_TURNS_BEFORE_SUMMARY=10
KEEP_RECENT_TURNS=2

# Speaker Diarization & Voice Locking Configuration
SPEECHMATIC_API_KEY=your_speechmatics_api_key
ENABLE_SPEAKER_DIARIZATION=true
SPEAKER_SENSITIVITY=0.5
MAX_SPEAKERS=5
ENABLE_VOICE_LOCKING=true